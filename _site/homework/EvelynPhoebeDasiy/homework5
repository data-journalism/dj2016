    "for pageNum in range(1, 14):\n",
    "    \n",
    "    content = urllib2.urlopen(urlBase + str(pageNum)).read() #获取网页的html文本\n",
    "    soup = BeautifulSoup(content) \n",
    "    table = soup.find(\"table\", { \"id\" : \"report1\" })\n",
    "    \n",
    "    for row in table.findAll(\"tr\"):\n",
